nohup ./train -dir /zf18/ll5fy/lab/dataset -source YelpNew -crossV 1 -K 15 -iter 50 > ./output/YelpNew_HFT_15.output 2>&1 &
nohup ./train -dir /zf18/ll5fy/lab/dataset -source YelpNew -crossV 1 -K 20 -iter 50 > ./output/YelpNew_HFT_20.output 2>&1 &