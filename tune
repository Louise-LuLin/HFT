#!/bin/sh
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PWD/liblbfgs-1.10/lib/.libs/
nohup ./train -dir /zf18/ll5fy/lab/dataset -source StackOverflow -crossV 5 -K 5 -iter 50 > ./output/5_StackOverflow_HFT_5.output 2>&1 &
nohup ./train -dir /zf18/ll5fy/lab/dataset -source StackOverflow -crossV 5 -K 10 -iter 50 > ./output/5_StackOverflow_HFT_10.output 2>&1 &
nohup ./train -dir /zf18/ll5fy/lab/dataset -source StackOverflow -crossV 5 -K 15 -iter 50 > ./output/5_StackOverflow_HFT_15.output 2>&1 &
nohup ./train -dir /zf18/ll5fy/lab/dataset -source StackOverflow -crossV 5 -K 20 -iter 50 > ./output/5_StackOverflow_HFT_20.output 2>&1 &
nohup ./train -dir /zf18/ll5fy/lab/dataset -source StackOverflow -crossV 5 -K 25 -iter 50 > ./output/5_StackOverflow_HFT_25.output 2>&1 &
